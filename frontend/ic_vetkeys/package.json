{
    "name": "ic_vetkeys",
    "version": "0.1.0",
    "license": "Apache-2.0",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/lib/index.es.js",
            "types": "./dist/types/index.d.ts"
        },
        "./tools": {
            "import": "./dist/lib/tools/index.es.js",
            "types": "./dist/types/tools/index.d.ts"
        }
    },
    "module": "dist/lib/index.es.js",
    "typings": "dist/types/index.d.ts",
    "dependencies": {
        "@dfinity/agent": "^2.3.0",
        "@dfinity/candid": "^2.3.0",
        "@dfinity/principal": "^2.3.0",
        "idb-keyval": "^6.2.1"
    },
    "devDependencies": {
        "@dfinity/identity": "^2.2.0",
        "@eslint/js": "^9.22.0",
        "@vitest/coverage-v8": "^3.0.5",
        "eslint": "^9.22",
        "eslint-plugin-prettier": "^5.2.5",
        "fake-indexeddb": "^6.0.0",
        "isomorphic-fetch": "3.0.0",
        "prettier": "^3.5.3",
        "typedoc": "^0.28.3",
        "typescript": "^5.7.3",
        "typescript-eslint": "8.27",
        "vite": "^6.1.3",
        "vite-plugin-dts": "^4.5.3",
        "vitest": "^3.0.5"
    },
    "scripts": {
        "build": "tsc && vite build",
        "coverage": "vitest run --coverage",
        "lint": "eslint",
        "make:docs": "npm run make:docs/index && npm run make:docs/tools",
        "make:docs/index": "typedoc src/index.ts --out ../../docs --excludeInternal",
        "make:docs/tools": "typedoc src/tools/index.ts --out ../../docs/tools --excludeInternal",
        "prettier": "prettier --write .",
        "prettier-check": "prettier --check .",
        "test": "vitest"
    }
}
